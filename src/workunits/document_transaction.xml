<workunit>
    <invokers>
        <invoker folderid="/explorer/txn/dtstransaction" caption="DTS Transaction" action="init" target="window"  index="2" role="SHARED"/>        
        <invoker folderid="/home/dts" caption="DTS Transaction" action="init" target="window"  index="2" role="SHARED"/>
        <invoker type="formActions" caption="Outgoing" mnemonic="o" shortcut="F1" action="outgoing" visibleWhen="#{mode=='INIT'}" name="outgoing" index="2001"/>
        <invoker type="formActions" caption="Send Document" mnemonic="s" shortcut="F2" action="send" visibleWhen="#{mode=='INIT'}" name="send" index="2002"/>
        <invoker type="formActions" caption="Receive Document" mnemonic="r" shortcut="F3" action="receive" visibleWhen="#{mode=='INIT'}" name="receive" index="2003"/>
        <invoker type="formActions" caption="Archive Document" mnemonic="a" shortcut="F4" action="archived" visibleWhen="#{mode=='INIT'}" name="archived" index="2004"/>
        <invoker type="formActions" caption="Send to Outgoing" action="save" visibleWhen="#{mode=='outgoing' }" index="2004"/>
        <invoker type="formActions" caption="Send" action="save" visibleWhen="#{mode=='send' &amp;&amp; !completed}" index="2004"/>
        <invoker type="formActions" caption="Receive" action="save" visibleWhen="#{mode=='receive' &amp;&amp; !completed}" index="2004"/>
        <invoker type="formActions" caption="Send to Archive" action="save" visibleWhen="#{mode=='archived' &amp;&amp; !completed}" index="2004"/>
        <invoker type="formActions" caption="Reset" mnemonic="r" shortcut="F5" action="init" visibleWhen="#{mode!='INIT' &amp;&amp; !completed}" index="2005" />
        <invoker type="formActions" action="print" caption="Print" mnemonic="p" visibleWhen="#{completed}" />
        <invoker type="formActions" action="createnew" caption="New" visibleWhen="#{completed}" mnemonic="n"/>
    </invokers>
    <code class="tagabukid.subay.models.OnlineTransactionController"/>
    <pages>
        <page template="tagabukid.subay.views.OnlineTransactionPage"/>
        <page name="completed" template="tagabukid.subay.views.OnlineTransactionInfoPage"/>
    </pages>
    
</workunit>